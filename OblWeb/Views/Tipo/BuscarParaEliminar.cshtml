@using ObligatorioP3WebApplication.Models
@model TipoModel

@if (Model != null)
{
    <h3>Estas seguro que deseas eliminar este tipo de cabaña?</h3>
    <div>
        <h4>Tipo</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.nombre)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.nombre)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.descripcion)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.descripcion)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.costoXHuesped)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.costoXHuesped)
            </dd>
        </dl>
        @Html.ActionLink("Confirmar" , "Delete",  new { nombre = Model.nombre}) <br /> <br />
        <a asp-action="ObtenerPorNombre" class="list-unstyled">Volver</a>
    </div>
}

<a asp-controller="Tipo" asp-action="ObtenerPorNombre">Volver</a>