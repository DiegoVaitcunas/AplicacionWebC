@model MantenimientoModel[]

@{
    ViewData["Title"] = "VerMantenimientosDeCabañasEntreCapListar";
}

<h1>VerMantenimientosDeCabañasEntreCapListar</h1>

@if (Model != null && Model.Count() > 0)
{
	<table class="table">
		<thead>
			<tr>
				<th>
					Costo
				</th>
				<th>
					Nombre del trabajador
				</th>
				<th></th>
			</tr>
		</thead>
		<tbody>
			@foreach (var item in Model)
			{
				<tr>
					<td>
						@Html.DisplayFor(modelItem => item.Costo)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.NombreTrabajador)
					</td>
				</tr>
			}
		</tbody>
	</table>
	<br />
}
else
{
	<p>No se encontraron mantenimientos para cabañas con esas capacidades</p>
}
<a asp-controller="Cabana" asp-action="VerMantenimientosDeCabañasEntreCap">Volver</a>